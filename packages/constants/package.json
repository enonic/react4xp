{
  "name": "react4xp-buildconstants",
  "version": "0.8.3",
  "description": "React4xp helper: creates a JSON file with shared constants that define and setup a React4xp project.",
  "main": "index.js",
  "files": [
    "bin/cli.js",
    "ensureTargetOutputPath.js",
    "index.js",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "react4xp-buildconstants": "bin/cli.js"
  },
  "scripts": {
    "compile": "node_modules/babel-cli/bin/babel.js src -d . --no-comments",
    "lint": "node_modules/eslint/bin/eslint.js src/",
    "test:mocha": "NODE_PATH=./ node_modules/mocha/bin/mocha --compilers js:babel-core/register --require babel-polyfill --require test/test_helper.js \"test/**/*@(spec.js)\" && mkdir test/output/cli; bin/cli.js test/output/cli && node_modules/path-exists-cli/cli.js test/output/cli/react4xp_constants.json && node_modules/path-exists-cli/cli.js test/output/cli/build/resources/main/lib/enonic/react4xp/react4xp_constants.json",
    "nsi:install": "npm-safe-install",
    "prepublishOnly": "cd ../.. && ./gradlew :packages:constants:buildAll --info",
    "postpublishOnly": "cd ../.. && ./gradlew :packages:constants:cleanBuilt && ./copytag.js constants"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:enonic/react4xp-npm.git"
  },
  "author": "Espen Norderud",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/enonic/lib-react4xp/issues"
  },
  "homepage": "https://github.com/enonic/react4xp-build-constants",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "3.5.0",
    "copyfiles": "^2.1.0",
    "deep-freeze": "^0.0.1",
    "eslint": "4.18.2",
    "eslint-plugin-react": "^7.12.4",
    "mocha": "^5.2.0",
    "npm-safe-install": "^1.1.0",
    "path-exists-cli": "^1.0.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  }
}
