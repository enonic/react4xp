{
  "name": "react4xp-mono",
  "version": "1.0.0",
  "description": "React4xp monorepo for all NPM dependencies",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "_TEMP_DISABLED_version": "npm run test && lerna version --conventional-commits --exact && lerna run postversion && echo \"Versioning done, using conventional commits. Publish to NPM with: 'gradlew doPublish'.\"",
    "setup": "npm i && npm run cleanNpm && npm run npmInstall",
    "npmInstall": "run-script-os",
    "npmInstall:win32": "gradlew npmInstall --info",
    "npmInstall:default": "./gradlew npmInstall --info",
    "clean": "run-script-os",
    "clean:win32": "gradlew cleanBuilt --info",
    "clean:default": "./gradlew cleanBuilt --info",
    "cleanNpm": "run-script-os",
    "cleanNpm:win32": "gradlew cleanNpm --info",
    "cleanNpm:default": "./gradlew cleanNpm --info",
    "test": "run-script-os",
    "test:win32": "gradlew test --info",
    "test:default": "./gradlew test --info",
    "build": "run-script-os",
    "build:win32": "gradlew build --info",
    "build:default": "./gradlew build --info",
    "npmLink": "run-script-os",
    "npmLink:win32": "gradlew npmLink --info",
    "npmLink:default": "./gradlew npmLink --info"
  },
  "author": "Espen Norderud",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-transform-arrow-functions": "^7.13.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/register": "^7.13.8",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "chai": "^3.5.0",
    "copyfiles": "^2.4.1",
    "core-js": "^3.9.1",
    "deep-freeze": "0.0.1",
    "duckfactory": "^1.3.5",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^1.3.1",
    "lerna": "^3.22.1",
    "lint-staged": "^8.2.1",
    "mocha": "^8.3.2",
    "npm-safe-install": "^1.1.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react-redux": "^6.0.1",
    "redux": "^4.0.5",
    "run-script-os": "^1.1.5",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:enonic/react4xp-npm.git"
  },
  "dependencies": {}
}
